import java.io.*;

boolean hasError = false;

File build = new File(basedir, "build.log");
BufferedReader br = new BufferedReader(new FileReader(build));
for(;;) {
  String line = br.readLine();
  if(line == null) {
    break;
  }
  if(line.indexOf("ERROR") >= 0) {
    hasError = true;
  }
}
br.close();

return !hasError;
